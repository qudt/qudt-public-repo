quantitykind:CompletedCallDensity
  a qudt:InformationTheoryQuantityKind ;
  a qudt:QuantityKind ;
  qudt:applicableUnit unit:PER-SEC ;
  dcterms:description "\"Completed Call Density\" is the call density for the call attempts that result in the transmission of an answer signal."^^rdf:HTML ;
