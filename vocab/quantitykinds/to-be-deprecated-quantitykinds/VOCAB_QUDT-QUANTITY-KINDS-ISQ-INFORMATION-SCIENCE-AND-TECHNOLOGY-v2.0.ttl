  a qudt:QuantityKind ;
  qudt:applicableUnit unit:NAT ;
  qudt:applicableUnit unit:SHANNON ;
  dcterms:description "\"Maximum Entropy} occurs when $p(x_i) = 1/n$ for $i = 1, ..., n$. Maximum entropy is sometimes called \\emph{decision content\" because the value is the same when the base is an integer, for the same number of events."^^rdf:HTML ;
  qudt:symbol "H_0" ;
