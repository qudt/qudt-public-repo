qudt:RULE_GSP-008
  a qudt:Rule ;
  dcterms:description "Symbols for the elements are normally printed in Roman type without regard to the type used in the surrounding text (see NIST \\ SP\\ 811 \\href{http://physics.nist.gov/Pubs/SP811/sec10.html#10.2.3}{section 10.2.3)}. They are not followed by a period unless at the end of a sentence."^^rdf:HTML ;
  dcterms:title "Typeface and punctuation for element symbols" ;
  qudt:id "R-GSP-008" ;
  qudt:informativeReference "http://physics.nist.gov/Pubs/SP811/sec10.html#10.4.1"^^xsd:anyURI ;
