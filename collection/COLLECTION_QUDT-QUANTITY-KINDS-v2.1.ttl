qudt:FindAllUnitsNotInSystemOfUnits
  a qudt:Query ;
  spin:query [
      a sp:Select ;
      sp:where (
          [
            a sp:TriplePath ;
            sp:object qudt:Unit ;
            sp:path [
                a sp:ModPath ;
                sp:modMax -2 ;
                sp:modMin 0 ;
                sp:subPath rdfs:subClassOf ;
                rdfs:isDefinedBy <http://qudt.org/2.1/collection/quantitykinds> ;
              ] ;
            sp:subject [
                sp:varName "uC" ;
              ] ;
            rdfs:isDefinedBy <http://qudt.org/2.1/collection/quantitykinds> ;
          ]
          [
            sp:object [
                sp:varName "uC" ;
              ] ;
            sp:predicate rdf:type ;
            sp:subject [
                sp:varName "u" ;
              ] ;
          ]
          [
            a sp:NotExists ;
            sp:elements (
                [
                  sp:object qudt:SOU_CGS-MECHANICS ;
                  sp:predicate qudt:belongsToSystemOfUnits ;
                  sp:subject [
                      sp:varName "u" ;
                    ] ;
                ]
              ) ;
            rdfs:isDefinedBy <http://qudt.org/2.1/collection/quantitykinds> ;
          ]
          [
            sp:object [
                sp:varName "u" ;
              ] ;
            sp:predicate rdf:first ;
            sp:subject [
                sp:varName "?0" ;
              ] ;
          ]
          [
            sp:object [
                sp:varName "?1" ;
              ] ;
            sp:predicate rdf:rest ;
            sp:subject [
                sp:varName "?0" ;
              ] ;
          ]
          [
            sp:object rdf:type ;
            sp:predicate rdf:first ;
            sp:subject [
                sp:varName "?1" ;
              ] ;
          ]
          [
            sp:object [
                sp:varName "?2" ;
              ] ;
            sp:predicate rdf:rest ;
            sp:subject [
                sp:varName "?1" ;
              ] ;
          ]
          [
            sp:object [
                sp:varName "uC" ;
              ] ;
            sp:predicate rdf:first ;
            sp:subject [
                sp:varName "?2" ;
              ] ;
          ]
          [
            sp:object () ;
            sp:predicate rdf:rest ;
            sp:subject [
                sp:varName "?2" ;
              ] ;
          ]
          [
            sp:object [
                sp:varName "g" ;
              ] ;
            sp:predicate <http://www.topbraid.org/tops#statementDefinedIn> ;
            sp:subject [
                sp:varName "?0" ;
              ] ;
          ]
          [
            a sp:Filter ;
            sp:expression [
                a sp:eq ;
                sp:arg1 [
                    sp:varName "g" ;
                  ] ;
                sp:arg2 "http://qudt.org/2.0/vocab/unit/cgs/mechanics" ;
                rdfs:isDefinedBy <http://qudt.org/2.1/collection/quantitykinds> ;
              ] ;
            rdfs:isDefinedBy <http://qudt.org/2.1/collection/quantitykinds> ;
          ]
        ) ;
      rdfs:isDefinedBy <http://qudt.org/2.1/collection/quantitykinds> ;
    ] ;
  rdfs:isDefinedBy <http://qudt.org/2.1/collection/quantitykinds> ;
  rdfs:label "Find all units not in system of units" ;
