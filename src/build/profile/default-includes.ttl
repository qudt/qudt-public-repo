@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix profile: <http://qudt.org/schema/vocab/profile/> .
@prefix qudt: <http://qudt.org/schema/qudt/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .

profile:defaultIncludesShape
  a sh:NodeShape ;
  sh:property [
    sh:path qudt:includes ;
    sh:values [
      sh:path ( qudt:includes [
        sh:alternativePath ( qudt:applicableUnit qudt:hasQuantityKind qudt:scalingOf ( 
          qudt:hasFactorUnit qudt:hasUnit ) ) ;
      ] ) ;
    ] ;
  ] ;
  sh:targetClass qudt:Profile .


