@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix qfn: <http://qudt.org/shacl/functions#> .
@prefix qudt: <http://qudt.org/schema/qudt/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .

qfn:validateSymbols
  a owl:Ontology ;
  owl:imports qfn: .

qfn:symbolRule
  a sh:NodeShape ;
  sh:severity sh:Violation ;
  sh:sparql [
    a sh:SPARQLConstraint ;
    sh:message """
    Unit {$this} has symbol '{?actualSymbol}', but '{?calculatedSymbol}' is inferred via {?sourceRelation}.
    If the current symbol is correct, add the unit to `src/build/sparql2shacl/symbol/units-overriding-inferred-symbols.ttl`
    """ ;
    sh:prefixes qfn: ;
    sh:select """
    SELECT * WHERE
    {
        {


{{QUERY_WITHOUT_PREFIXES}}


        }

        FILTER NOT EXISTS {
            qudt:UnitsOverridingInferredSymbol qudt:includes ?this .
        }
    }

    """ ;
  ] ;
  sh:targetClass qudt:Unit .


