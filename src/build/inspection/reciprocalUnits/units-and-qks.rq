SELECT * WHERE {

     GRAPH <dist:vocab:VOCAB_QUDT-UNITS-ALL.ttl> {
        ?unit qudt:hasReciprocalUnit ?reciprocalUnit ;
     }

    GRAPH <dist:vocab:VOCAB_QUDT-QUANTITY-KINDS-ALL.ttl> {
        ?qk qudt:applicableUnit ?unit .
        ?reciprocalQk qudt:applicableUnit ?reciprocalUnit .
    }


} ORDER BY ?qk ?reciprocalQk ?unit ?reciprocalUnit