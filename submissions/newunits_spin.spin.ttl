newunits_spin:replaceConversionInDVTemplate
  a spin:Function ;
  spin:body [
      a sp:Select ;
      sp:resultVariables (
          [
            sp:varName "componentDVResult" ;
          ]
        ) ;
      sp:where (
          [
            a sp:Bind ;
            sp:expression [
                a spif:indexOf ;
                sp:arg1 spin:_arg1 ;
                sp:arg2 spin:_arg2 ;
              ] ;
            sp:variable [
                sp:varName "typeIndex" ;
              ] ;
          ]
          [
            a sp:Bind ;
            sp:expression [
                a fn:substring ;
                sp:arg1 spin:_arg1 ;
                sp:arg2 [
                    a sp:add ;
                    sp:arg1 [
                        sp:varName "typeIndex" ;
                      ] ;
                    sp:arg2 2 ;
                  ] ;
                sp:arg3 1 ;
              ] ;
            sp:variable [
                sp:varName "typeNext" ;
              ] ;
          ]
          [
            a sp:Bind ;
            sp:expression [
                a fn:substring ;
                sp:arg1 spin:_arg1 ;
                sp:arg2 [
                    a sp:add ;
                    sp:arg1 [
                        sp:varName "typeIndex" ;
                      ] ;
                    sp:arg2 2 ;
                  ] ;
                sp:arg3 2 ;
              ] ;
            sp:variable [
                sp:varName "typePower" ;
              ] ;
          ]
          [
            a sp:Bind ;
            sp:expression [
                a fn:concat ;
                sp:arg1 spin:_arg2 ;
                sp:arg2 [
                    a sp:if ;
                    sp:arg1 [
                        a sp:or ;
                        sp:arg1 [
                            a sp:eq ;
                            sp:arg1 [
                                sp:varName "typeNext" ;
                              ] ;
                            sp:arg2 " " ;
                          ] ;
                        sp:arg2 [
                            a sp:eq ;
                            sp:arg1 [
                                sp:varName "typeNext" ;
                              ] ;
                            sp:arg2 "" ;
                          ] ;
                      ] ;
                    sp:arg2 1 ;
                    sp:arg3 [
                        a sp:if ;
                        sp:arg1 [
                            a sp:eq ;
                            sp:arg1 [
                                sp:varName "typeNext" ;
                              ] ;
                            sp:arg2 "-" ;
                          ] ;
                        sp:arg2 [
                            sp:varName "typePower" ;
                          ] ;
                        sp:arg3 [
                            sp:varName "typeNext" ;
                          ] ;
                      ] ;
                  ] ;
              ] ;
            sp:variable [
                sp:varName "typeDVComponent" ;
              ] ;
          ]
          [
            a sp:Bind ;
            sp:expression [
                a sp:if ;
                sp:arg1 [
                    a sp:bound ;
                    sp:arg1 [
                        sp:varName "typeIndex" ;
                      ] ;
                  ] ;
                sp:arg2 [
                    a spif:replaceAll ;
                    sp:arg1 spin:_arg4 ;
                    sp:arg2 spin:_arg3 ;
                    sp:arg3 [
                        sp:varName "typeDVComponent" ;
                      ] ;
                  ] ;
                sp:arg3 spin:_arg4 ;
              ] ;
            sp:variable [
                sp:varName "componentDVResult" ;
              ] ;
          ]
        ) ;
    ] ;
  spin:constraint [
      a spl:Argument ;
      spl:predicate sp:arg1 ;
      spl:valueType xsd:string ;
    ] ;
  spin:constraint [
      a spl:Argument ;
      spl:predicate sp:arg2 ;
      spl:valueType xsd:string ;
    ] ;
  spin:constraint [
      a spl:Argument ;
      spl:predicate sp:arg3 ;
      spl:valueType xsd:string ;
      rdfs:comment "The DV template component, such as \"A0\"" ;
    ] ;
  spin:constraint [
      a spl:Argument ;
      spl:predicate sp:arg4 ;
      spl:valueType xsd:string ;
      rdfs:comment "The DV template (or current modification of it)" ;
    ] ;
  rdfs:comment """Given a converted unit string (e.g., \"L M T-1\",
          a type (e.g., \"L\"),
          a template component (e.g., \"L0\"), and
          a template (e.g., \"A0 E0 L0 I0 M0 H0 T0 D0\")
Return the template modified for each term int he unit string (in this example: \"A0 E0 L1 I0 M1 H0 T-1 D0\"

TEST:

SELECT ?aresult
WHERE {
    BIND (\"A0 E0 L0 I0 M0 H0 T0 D0\" AS ?template) .
    ?unit a newUnits3:Export .
    BIND (newunits_spin:convertDimensions(?unit) AS ?convresult) .
    BIND (newunits_spin:replaceConversionInDVTemplate(?convresult, \"A\", \"A0\", ?template) AS ?aresult) .
}""" ;
