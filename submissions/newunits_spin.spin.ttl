newunits_spin:convertDimensions
  a spin:Function ;
  spin:body [
      a sp:Select ;
      sp:resultVariables (
          [
            sp:varName "convertedUnitDIMS" ;
          ]
        ) ;
      sp:where (
          [
            sp:object [
                sp:varName "IEC_UNIT" ;
              ] ;
            sp:predicate newUnits3:primaryUnit ;
            sp:subject spin:_arg1 ;
          ]
          [
            a sp:Bind ;
            sp:expression [
                a fn:upper-case ;
                sp:arg1 [
                    sp:varName "IEC_UNIT" ;
                  ] ;
              ] ;
            sp:variable [
                sp:varName "IEC_UC_UNIT" ;
              ] ;
          ]
          [
            sp:object [
                sp:varName "?0" ;
              ] ;
            sp:predicate spif:split ;
            sp:subject [
                sp:varName "elem" ;
              ] ;
          ]
          [
            sp:object [
                sp:varName "IEC_UC_UNIT" ;
              ] ;
            sp:predicate rdf:first ;
            sp:subject [
                sp:varName "?0" ;
              ] ;
          ]
          [
            sp:object [
                sp:varName "?1" ;
              ] ;
            sp:predicate rdf:rest ;
            sp:subject [
                sp:varName "?0" ;
              ] ;
          ]
          [
            sp:object " " ;
            sp:predicate rdf:first ;
            sp:subject [
                sp:varName "?1" ;
              ] ;
          ]
          [
            sp:object () ;
            sp:predicate rdf:rest ;
            sp:subject [
                sp:varName "?1" ;
              ] ;
          ]
          [
            a sp:Bind ;
            sp:expression [
                a spif:replaceAll ;
                sp:arg1 [
                    a spif:replaceAll ;
                    sp:arg1 [
                        a spif:replaceAll ;
                        sp:arg1 [
                            a spif:replaceAll ;
                            sp:arg1 [
                                a spif:replaceAll ;
                                sp:arg1 [
                                    a spif:replaceAll ;
                                    sp:arg1 [
                                        a spif:replaceAll ;
                                        sp:arg1 [
                                            a spif:replaceAll ;
                                            sp:arg1 [
                                                a spif:replaceAll ;
                                                sp:arg1 [
                                                    sp:varName "IEC_UC_UNIT" ;
                                                  ] ;
                                                sp:arg2 "RAD" ;
                                                sp:arg3 "D" ;
                                              ] ;
                                            sp:arg2 "A" ;
                                            sp:arg3 "E" ;
                                          ] ;
                                        sp:arg2 "MOL" ;
                                        sp:arg3 "A" ;
                                      ] ;
                                    sp:arg2 "M" ;
                                    sp:arg3 "L" ;
                                  ] ;
                                sp:arg2 "CD" ;
                                sp:arg3 "I" ;
                              ] ;
                            sp:arg2 "KG" ;
                            sp:arg3 "M" ;
                          ] ;
                        sp:arg2 "K" ;
                        sp:arg3 "H" ;
                      ] ;
                    sp:arg2 "SR" ;
                    sp:arg3 "D" ;
                  ] ;
                sp:arg2 "S" ;
                sp:arg3 "T" ;
              ] ;
            sp:variable [
                sp:varName "convertedUnitDIMS" ;
              ] ;
          ]
        ) ;
    ] ;
  spin:constraint [
      a spl:Argument ;
      spl:predicate sp:arg1 ;
      spl:valueType xsd:string ;
    ] ;
  rdfs:comment """Convert something like m2 s-1 into L T-1

1. Convert LC to UC
2. Match and replace types (add one at a time)
    a. Aggregate like terms (e.g., K and K-1)
    b. Sum exponents (e.g., 1 -1 = 0) -> K0 D1
    c. S -> T, KG -> M, M -> L, A -> E, CD -> I, SR -> L, MOL -> A, K -> H, RAD -> L,
3. Remove spaces (easy)

CALLED BY: newunits_spin:getDimensionComponents

REAL TEST:

SELECT ?result
WHERE {
  ?arg1 a newUnits3:Export .
  BIND (newunits_spin:convertDimensions(?arg1) AS ?result) .
}

FULL SPARQL TEST:

SELECT ?IEC_UNIT ?result
WHERE {
    ?arg1 a newUnits3:Export .
    ?arg1 newUnits3:primaryUnit ?IEC_UNIT .
    BIND (fn:upper-case(?IEC_UNIT) AS ?IEC_UC_UNIT) .
    ?elem spif:split (?IEC_UC_UNIT \" \") .
    BIND (spif:replaceAll(
                   spif:replaceAll(
                       spif:replaceAll(
                           spif:replaceAll(
                               spif:replaceAll(
                                   spif:replaceAll(
                                       spif:replaceAll(
                                           spif:replaceAll(
                                               spif:replaceAll(?IEC_UC_UNIT, \"RAD\", \"D\"),
                                               \"A\", \"E\"),
                                           \"MOL\", \"A\"),
                                       \"M\", \"L\"),
                                   \"CD\", \"I\"),
                               \"KG\", \"M\"),
                           \"K\", \"H\"),
                       \"SR\", \"D\"),
                   \"S\", \"T\") AS ?result) .
}

Send the result into a function that will break the result up and rewrite the template based on the items in it""" ;
  rdfs:subClassOf spin:Functions ;
.
