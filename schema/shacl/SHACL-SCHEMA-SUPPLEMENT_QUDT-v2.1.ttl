qudt:SymbolAndExpressionDisjointPropertiesConstraint
  a sh:NodeShape ;
  rdfs:label "Symbol and expression disjoint properties constraint" ;
  sh:sparql [
      rdfs:comment "Checks that a unit or quantitykind does not have both a symbol and an expression" ;
      sh:message "A unit or quantity kind must not have both a symbol and an expression." ;
      sh:prefixes <http://qudt.org/2.1/shacl/schema/overlay/qudt> ;
      sh:select """SELECT $this
WHERE {
	$this qudt:symbol ?symbol .
	$this qudt:expression ?expression .
	
}""" ;
    ] ;
  sh:targetClass qudt:QuantityKind ;
  sh:targetClass qudt:Unit ;
.
qudt:UniqueSymbolPropertyConstraint
  a sh:NodeShape ;
  rdfs:label "Unique symbol property constraint" ;
  sh:sparql [
      rdfs:comment "Checks that a unit or quantity kind has a unique symbol" ;
      sh:message "Unit or Quantity Kind has non-unique symbol conflicts with '{?another}'" ;
      sh:prefixes <http://qudt.org/2.1/shacl/schema/overlay/qudt> ;
      sh:select """SELECT $this ?another
WHERE {{
	$this qudt:symbol ?symbol .
     ?another qudt:symbol ?symbol .
    FILTER (?another != $this) .}
?another a ?class .
    ?unitClass rdfs:subClassOf* qudt:Unit .
    ?qkClass rdfs:subClassOf* qudt:QuantityKind .
FILTER (?class = ?unitClass || ?class = ?qkClass) .
}""" ;
    ] ;
  sh:targetClass qudt:QuantityKind ;
  sh:targetClass qudt:Unit ;
.
